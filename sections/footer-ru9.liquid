          uResolution: { value: new Vec2() },
          uHueShift: { value: {{ section.settings.dark_veil_hue_shift | times: 36 | divided_by: 10 | default: 0 }} },
          uNoise: { value: {{ section.settings.dark_veil_noise | default: 0 }} },
          uScan: { value: {{ section.settings.dark_veil_scan | default: 0 }} },
          uScanFreq: { value: {{ section.settings.dark_veil_scan_freq | default: 0 }} },
          uWarp: { value: {{ section.settings.dark_veil_warp | default: 0 }} }
        }
      });
      var mesh = new Mesh(gl, { geometry: geometry, program: program });
      function resize() {
        var parent = canvas.parentElement;
        if (!parent) return;
        var scale = {{ section.settings.dark_veil_resolution_scale | default: 1 }};
        var w = parent.clientWidth, h = parent.clientHeight;
        renderer.setSize(Math.max(1, w * scale), Math.max(1, h * scale));
        program.uniforms.uResolution.value.set(w, h);
      }
      window.addEventListener('resize', resize);
      resize();
      var start = performance.now();
      var frame;
      function loop() {
        if (!canvas.isConnected) { cancelAnimationFrame(frame); return; }
        program.uniforms.uTime.value = ((performance.now() - start) / 1000) * {{ section.settings.dark_veil_speed | default: 0.5 }};
        renderer.render({ scene: mesh });
        frame = requestAnimationFrame(loop);
      }
      try { loop(); } catch (e) { console.error('[DarkVeil] render loop error', e); }
      canvas._darkveil_cleanup = function() { cancelAnimationFrame(frame); window.removeEventListener('resize', resize); };
    }

    function loadAndInit() {
      if (window.ogl || window.OGL) { initDarkVeil(); return; }
      var triedJsDelivr = false;
      function load(src) {
        var s = document.createElement('script');
        s.src = src;
        s.async = true;
        s.onload = initDarkVeil;
        s.onerror = function(){
          if (!triedJsDelivr) {
            triedJsDelivr = true;
            load('https://cdn.jsdelivr.net/npm/ogl@0.0.103/dist/ogl.min.js');
          } else {
            console.warn('[DarkVeil] Failed to load OGL from CDNs. Using CSS fallback.');
            var root = document.querySelector('.footer-ru9');
            if (root) root.setAttribute('data-darkveil-fallback', '1');
          }
        };
        document.head.appendChild(s);
      }
      load('https://unpkg.com/ogl@0.0.103/dist/ogl.min.js');
    }
    loadAndInit();
  })();
  </script>
  {%- endif -%}
</div>

