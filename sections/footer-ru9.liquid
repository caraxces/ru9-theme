      body.sticky-atc-is-active .footer-ru9-fab-wrapper[data-section-id="{{ section.id }}"] {
        bottom: 220px !important;
      }
    } {% endcomment %}
    
    @media screen and (max-width: 767px){
     /* Position fab-contact-toggle above product__stick-product-wrap with 20px gap */
     .footer-ru9-fab-wrapper[data-section-id="{{ section.id }}"] {
       bottom: 58px !important; /* 48px (product__stick-product-wrap height) + 20px gap - 10px adjustment */
     }
     
     {%if section.settings.hotline_enable -%}
       .footer-ru9-fab-wrapper[data-section-id="{{ section.id }}"] {
          bottom: 90px!important;
      }
     {%endif-%}
     .backtop-show .footer-ru9-fab-wrapper[data-section-id="{{ section.id }}"] {
      bottom: 150px !important;
     }
    }
