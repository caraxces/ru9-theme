<script type="lazyload2" >
  $(document).ready(function() { 

    $('.slider-for').slick({
      slidesToShow: 1,
      slidesToScroll: 1,
      arrows: false,
      fade: true,
      asNavFor: '.slider-nav'
    });
    $('.slider-nav').slick({
      slidesToShow: 3,
      slidesToScroll: 1,
      asNavFor: '.slider-for',
      dots: true,
      centerMode: true,
      focusOnSelect: true
    });
    setTimeout(function() { 
      var tabItemHeight = jQuery('div.tabs_item').height();
      jQuery('div.tab_content').css('min-height',tabItemHeight);
    }, 1000);
    function pfTab($) { 
      jQuery('.tab ul.tabs').addClass('active').find('> li:eq(0)').addClass('current');
      jQuery('.tab .tab_content').find('> .tabs_item:eq(0)').addClass('active');
      jQuery('.tab ul.tabs li a').click(function (g) { 

        var tab = jQuery(this).closest('.tab'), 
            index = jQuery(this).closest('li').index();
        tab.find('ul.tabs > li').removeClass('current');
        tab.find('.tab_content > .tabs_item').removeClass('active');
        jQuery(this).closest('li').addClass('current');
        tab.find('.tab_content').find('div.tabs_item').not('div.tabs_item:eq(' + index + ')').animate({opacity: 0}, 100 , function(){

          jQuery(this).css("visibility", "hidden");
          jQuery(this).removeClass('active');
        });
        tab.find('.tab_content').find('div.tabs_item:eq(' + index + ')').animate({opacity: 1}, 100 , function(){
          jQuery(this).css("visibility", "visible");
          jQuery(this).addClass('active');
          var tabItemHeight = tab.find('.tab_content').find('div.tabs_item:eq(' + index + ')').height();
          jQuery('.tab_content').css('min-height',tabItemHeight+'px;');
        });
        g.preventDefault();
      });
    }
    //pfTab();

  });
</script>

<script type="lazyload2" >
    $(document).ready(()=>{
  
                      $('.accordion-toggle').click(function(e) {
      e.preventDefault();
      var $this = $(this);
      if ($this.next().hasClass('pf-show')) {
        $this.removeClass('active');
        $this.next().removeClass('pf-show');
        $this.next().slideUp(350);
      } else {
        $this.next().toggleClass('pf-show');
        $this.toggleClass('active');
        $this.next().slideToggle(350);
      }
    });
  
    })
  
  </script>