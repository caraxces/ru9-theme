}

.col-4 {
    flex: 0 0 33.3333333333%;
    max-width: 33.3333333333%
}

.col-5 {
    flex: 0 0 41.6666666667%;
    max-width: 41.6666666667%
}

.col-6 {
    flex: 0 0 50%;
    max-width: 50%
}

.col-7 {
    flex: 0 0 58.3333333333%;
    max-width: 58.3333333333%
}

.col-8 {
    flex: 0 0 66.6666666667%;
    max-width: 66.6666666667%
}

.col-9 {
    flex: 0 0 75%;
    max-width: 75%
}

.col-10 {
    flex: 0 0 83.3333333333%;
    max-width: 83.3333333333%
}

.col-11 {
    flex: 0 0 91.6666666667%;
    max-width: 91.6666666667%
}

.col-12 {
    flex: 0 0 100%;
    max-width: 100%
}

@media (min-width:576px) {
    .col-sm {
        flex-basis: 0;
        flex-grow: 1;
        max-width: 100%
    }

    .col-sm-1 {
        flex: 0 0 8.3333333333%;
        max-width: 8.3333333333%
    }

    .col-sm-2 {
        flex: 0 0 16.6666666667%;
        max-width: 16.6666666667%
    }

    .col-sm-3 {
        flex: 0 0 25%;
        max-width: 25%
    }

    .col-sm-4 {
        flex: 0 0 33.3333333333%;
        max-width: 33.3333333333%
    }

    .col-sm-5 {
        flex: 0 0 41.6666666667%;
        max-width: 41.6666666667%
    }

    .col-sm-6 {
        flex: 0 0 50%;
        max-width: 50%
    }

    .col-sm-7 {
        flex: 0 0 58.3333333333%;
        max-width: 58.3333333333%
    }

    .col-sm-8 {
        flex: 0 0 66.6666666667%;
        max-width: 66.6666666667%
    }

    .col-sm-9 {
        flex: 0 0 75%;
        max-width: 75%
    }

    .col-sm-10 {
        flex: 0 0 83.3333333333%;
        max-width: 83.3333333333%
    }

    .col-sm-11 {
        flex: 0 0 91.6666666667%;
        max-width: 91.6666666667%
    }

    .col-sm-12 {
        flex: 0 0 100%;
        max-width: 100%
    }
}

.hidden {
    display: none
}


.main-color {
    color: var(--main-color)
}

.primary-color {
    color: #003349
}

.page-heading.h1,
h1.page-heading {
    font-size: 34px
}

@media (max-width:767px) {


    .mb-60 {
        padding-top: 5rem
    }
}
</style>
<style>
  #pagefly-rich-text-pf-mattress-rich-text {
    background-color: transparent;
  }
  body {
    background-color: #E1F1EF;
    background-image: url("{{'cloud_2.svg' | asset_url }}");
      background-position: center 100px;
      background-size: contain;
      background-repeat: no-repeat;
      }
  /*Accordion*/
  .pf-accordion .pf-accordion-header {
    position: relative;
    cursor: pointer;
    font-weight: bold; }
  .pf-accordion .pf-accordion-header:after {
    font-family: FontAwesome;
    font-size: 14px;
    content: "\f067";
    position: absolute;
    top: 50%;
    transform: translate(-50%, -50%);
    font-weight: normal; }
  .pf-accordion .pf-accordion-header.active:after {
    content: "\f068"; }
  .pf-accordion .accordion-content {
    padding: 15px;
    display: none; }

  .pf-accordion-style-1 .pf-accordion__item {
    margin-bottom: 25px; }
  .pf-accordion-style-1 .pf-accordion-header:after {
    right: 15px; }

  .pf-accordion-style-2 {
    max-width: 1100px;
    margin: 0 auto; }
  .pf-accordion-style-2 .pf-accordion {
    border-top: 1px solid #727272; }
  .pf-accordion-style-2 .pf-accordion__item {
    padding: 40px;
    border-bottom: 1px solid #727272; }
  .pf-accordion-style-2 .pf-accordion-header.active {
    color: var(--primary-color); }
  .pf-accordion-style-2 .pf-accordion-header:after {
    left: -15px; }

  .pf-accordion-style-3 .pf-accordion__item {
    padding: 25px 0;
    margin-bottom: 0; }
  .pf-accordion-style-3 .pf-accordion .pf-accordion-header {
    font-size: 22px; }
  .pf-accordion-style-3 .pf-accordion-header:after {
    right: 0; }
  .pf-accordion-style-3 .accordion__item-child {
    padding-left: 0;
    padding-right: 0; }
  .pf-accordion-style-3 .accordion__item-child .pf-accordion__item {
    padding: 25px 0; }
  .pf-accordion-style-3 .accordion__item-child .accordion-content {
    padding: 50px 80px 0; }
  @media(max-width: 767px){
    .pf-accordion-style-3 .accordion__item-child .pf-accordion-header {
    font-size: 16px; }
  .pf-accordion-style-3 .accordion__item-child .accordion-content {
    padding: 25px 20px; }
  }
</style>
